@model IEnumerable<mvcTest.Models.Shoptype>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Aname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ename)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Aliases)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ealiases)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Photo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Category)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Aname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ename)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Aliases)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ealiases)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Photo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Category.CategoryEname)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ShopTypeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ShopTypeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ShopTypeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
